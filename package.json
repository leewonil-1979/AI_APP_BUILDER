{
  "name": "ai_app_builder",
  "private": true,
  "workspaces": [
    "apps/*",
    "builder",
    "packages/*"
  ],
  "scripts": {
    "new": "tsx builder/scripts/new-app.ts",
    "deploy": "tsx builder/scripts/deploy.ts",
    "validate": "npm run -ws --if-present lint && npm run -ws --if-present typecheck && npm run -ws --if-present test || exit 0",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "ajv": "^8.17.1",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^3.0.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "dotenv": "^17.2.2"
  }
}
